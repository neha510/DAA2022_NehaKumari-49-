#include<iostream>
using namespace std;

int main()
{
       int arr[100];
       int n,i,key,count =0;
       scanf("%d",&n);
       
       printf("enter elements :");
       for(i=0; i<n; i++)
       {
              scanf("%d",&arr[i]);
       }
       printf("enter key :");
       scanf("%d",&key);
       for(i=0; i<n; i++)
       {
              if(arr[i] == key)
              {
                    count++; 
              }
       }
       if(count == 0)
       {
              printf("KEY NOT PRESENT !!");
       }
       else
       {
              printf("KEY PRESENT : %d copies",count);
       }
       return 0;
}
